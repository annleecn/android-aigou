<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="title_txt_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
    </style>
    
    <style name="main_tab_but_linear">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:clickable">true</item>
    </style>
    
    <style name="main_list_image">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:clickable">true</item>
    </style>
    
    <style name="main_list_image_2">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:clickable">true</item>
    </style>
    
    <style name="main_list_image_3">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:clickable">true</item>
    </style>
    
    <style name="MainUnderButTheme">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:clickable">true</item>
        <item name="android:button">@null</item> 
        <item name="android:ellipsize">marquee</item>
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:background">@drawable/main_btn_bg</item>
    </style>
    
    <style name="TitleTextTheme">
        <item name="android:textSize">@dimen/title_text_size</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="MineLoginEdit">
        <item name="android:textSize">@dimen/mine_login_textsize2</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/grey</item>
        <item name="android:background">@drawable/login_editbg</item>
        <item name="android:padding">@dimen/mine_login_editpad</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="MineOtherLogin">
        <item name="android:textSize">@dimen/mine_login_textsize3</item>
        <item name="android:textColor">@color/grey</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@drawable/login_otherbg</item>
        <item name="android:paddingTop">10.0dip</item>
        <item name="android:paddingBottom">10.0dip</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/mine_login_scale2</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    
    <style name="main_img_text">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_marginBottom">8dip</item>
    </style>
    
    <style name="collect_but">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:textSize">@dimen/collect_tab_ts</item>
        <item name="android:textColor">@color/collect_tab_tc_nosel</item>
        <item name="android:background">@drawable/collect_btnsel</item>
    </style>

</resources>














































